extends layout

block content
 .container
  .col-sm-10.col-sm-offset-1
   each blog in Blogs
    .panel.panel-default
     .panel-body
      .col-sm-3
       img.pull-left(style='width:100px;height:100px;' alt="",src=""+blog.BlogThumbnail,)
      .col-sm-7
       h2=blog.BlogTitle
       p=blog.BlogBody
      .col-sm-2
       form(method='post')
        button(value=''+blog._id,name='DelSubmit').btn.btn-danger.pull-right Delete
     
     
     
  